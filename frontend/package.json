{
  "name": "readarr",
  "version": "1.0.0",
  "description": "Readarr",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/Readarr/Readarr.git"
  },
  "author": "Team Readarr",
  "license": "GPL-3.0",
  "readmeFilename": "readme.md",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "tsc -p config/tsconfig.json && vite build --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "lint": "eslint src --config config/eslint.config.js",
    "lint:fix": "eslint src --config config/eslint.config.js --fix",
    "format": "prettier --config config/prettierrc.json --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --config config/prettierrc.json --check \"src/**/*.{ts,tsx,css,json}\"",
    "typecheck": "tsc -p config/tsconfig.json --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@floating-ui/react": "0.27.16",
    "@microsoft/signalr": "10.0.0",
    "@sentry/browser": "10.30.0",
    "classnames": "2.5.1",
    "clipboard": "2.0.11",
    "clsx": "2.1.1",
    "filesize": "11.0.13",
    "fuse.js": "7.1.0",
    "history": "5.3.0",
    "jquery": "3.7.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "mousetrap": "1.6.5",
    "next-themes": "0.4.6",
    "normalize.css": "8.0.1",
    "prop-types": "15.8.1",
    "qs": "6.14.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "9.2.0",
    "react-router": "7.10.1",
    "react-router-dom": "7.10.1",
    "redux": "^5",
    "redux-actions": "3.0.3",
    "redux-batched-actions": "0.5.0",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "3.1.0",
    "reselect": "5.1.1",
    "stacktrace-js": "2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/lodash": "4.17.21",
    "@types/node": "25.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/redux-actions": "2.6.5",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "9.39.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "rimraf": "6.1.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vite": "7.2.7"
  },
  "volta": {
    "node": "20.11.1",
    "yarn": "1.22.19"
  }
}
